<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zagelx">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.CALL_PHONE"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!-- we need to include api key inside manifest -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <!--
             The API key for Google Maps-based APIs.
        -->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".Authentication.SpalshActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TripsPackage.AddTripsActivity"
            android:parentActivityName=".TripsPackage.TripsActivity"
            android:windowSoftInputMode="stateHidden">

            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".TripsPackage.TripsActivity" />
        </activity>
        <activity
            android:name=".OrdersPackage.AddOrdersActivity"
            android:parentActivityName=".OrdersPackage.OrdersActivity"
            android:windowSoftInputMode="stateHidden">
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".OrdersPackage.OrdersActivity" />
        </activity>

        <activity android:name=".TripsPackage.TripsActivity"></activity>

        <activity android:name=".OrdersPackage.OrdersActivity"></activity>

        <activity android:name=".Authentication.AfterRegisterUserInfo"></activity>

        <activity
            android:name=".OrdersPackage.AddOrdersMapActivity"
            android:parentActivityName=".OrdersPackage.OrdersActivity">
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".OrdersPackage.OrdersActivity" />
        </activity>

        <activity android:name=".OrdersPackage.TestAddress"></activity>

        <activity
            android:name=".UserInfo.ProfileActivity"
            android:parentActivityName=".OrdersPackage.OrdersActivity">
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".OrdersPackage.OrdersActivity" />
        </activity>

        <activity
            android:name=".UserInfo.AboutUsActivity"
            android:parentActivityName=".OrdersPackage.OrdersActivity">
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".OrdersPackage.OrdersActivity" />
        </activity>

        <activity
            android:name=".UserInfo.NotificationsActivity"
            android:parentActivityName=".OrdersPackage.OrdersActivity">
            <intent-filter>
                <action android:name=".UserInfo.Notifications" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".OrdersPackage.OrdersActivity" />
        </activity>
        <activity
            android:name=".DashboardPackage.DashboardActivity"
            android:parentActivityName=".OrdersPackage.OrdersActivity">
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".OrdersPackage.OrdersActivity" />
        </activity>
        <activity
            android:name=".TripsPackage.AddTripsMapActivity"
            android:parentActivityName=".TripsPackage.TripsActivity">
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".TripsPackage.TripsActivity" />
        </activity>
        <activity android:name=".OrdersPackage.OrderDetails" >
            <intent-filter>
                <action android:name=".OrdersPackage.OrderDetails" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".TripsPackage.TripsDetails" />
        <activity android:name=".OrdersPackage.OrdersRequestsActivity" />
        <activity android:name=".MerchantsDashboardPackage.MerchantsOrdersInside.NewOrdersInsideActivity" />
        <activity android:name=".MerchantsDashboardPackage.MerchantsOrdersInside.PickedOrdersInsideActivity" />
        <activity android:name=".MerchantsDashboardPackage.MerchantsOrdersInside.DeliveredOrdersInsideActivity" />
        <activity android:name=".MerchantsDashboardPackage.MerchantsOrdersInside.NegotiableOrdersInsideActivity" />
        <activity android:name=".MerchantsDashboardPackage.MerchantsOrdersInside.UnPickedOrdersInsideActivity" />
        <activity android:name=".MerchantsDashboardPackage.MerchantsOrdersInside.MerchantDashboardInsideActivity" />
        <activity android:name=".DashboardPackage.DelegateTripsActivity" />
        <activity android:name=".DashboardPackage.DelegateOrdersActivity" />
        <activity android:name=".DashboardPackage.DelegateDashboardActivity" />
        <activity android:name=".MainPackage.MainActivity" />
        <activity android:name=".MainPackage.MainTrips" />
        <activity android:name=".MainPackage.MainOrders" />
        <activity android:name=".UserInfo.SetAcurrateLocationActivity" />
        <activity android:name=".Authentication.NotVerifiedUser" />
        <activity android:name=".StaticBirdsDashboardPackage.StaticsOrdersPackage.StaticsOrdersDashboardActivity" />
        <activity android:name=".PMsDashboardPackage.PMInSideOrdersPackage.InsideOrdersDashboardActivity" />
        <activity android:name=".FreeBirdsDashboardPackage.FreesDashboardActivity" />
        <activity android:name=".FreeBirdsDashboardPackage.FreeDeliveredOrdersActivity" />
        <activity android:name=".FreeBirdsDashboardPackage.FreeNewOrdersActivity" />
        <activity android:name=".FreeBirdsDashboardPackage.FreePickedOrdersActivity" />
        <activity android:name=".FreeBirdsDashboardPackage.FreeUnPickedOrdersActivity" />
        <activity android:name=".StaticBirdsDashboardPackage.StaticsOrdersPackage.StaticHeadQuarOrdersActivity" />
        <activity android:name=".StaticBirdsDashboardPackage.StaticsOrdersPackage.StaticNewOrdersActivity" />
        <activity android:name=".StaticBirdsDashboardPackage.StaticsOrdersPackage.StaticPickedOrdersActivity" />
        <activity android:name=".StaticBirdsDashboardPackage.StaticsOrdersPackage.StaticUnPickedOrdersActivity" />
        <activity android:name=".StaticBirdsDashboardPackage.StaticsTripsPackage.StaticsTripsDashboardActivity" />
        <activity android:name=".PMsDashboardPackage.PMInSideOrdersPackage.InsideDeliveredOrdersActivity" />
        <activity android:name=".PMsDashboardPackage.PMInSideOrdersPackage.InsideNewOrdersActivity" />
        <activity android:name=".PMsDashboardPackage.PMInSideOrdersPackage.InsidePickedOrdersActivity" />
        <activity android:name=".PMsDashboardPackage.PMInSideOrdersPackage.InsideUnPickedOrdersActivity" />
        <activity android:name=".PMsDashboardPackage.PMOutSideOrdersPackage.OutsideOrdersDashboardActivity" />
        <activity android:name=".PMsDashboardPackage.PMOutSideOrdersPackage.OutSideOtherHeadQuarOrdersActivity" />
        <activity android:name=".PMsDashboardPackage.PMOutSideOrdersPackage.OutSideHeadQuarOrdersActivity" />
        <activity android:name=".PMsDashboardPackage.PMOutSideOrdersPackage.OutSideNewOrdersActivity" />
        <activity android:name=".PMsDashboardPackage.PMOutSideOrdersPackage.OutSidePickedOrdersActivity" />
        <activity android:name=".PMsDashboardPackage.PMOutSideOrdersPackage.OutSideUnPickedOrdersActivity" />
        <activity android:name=".PMsDashboardPackage.PMOutGoingTripsPackage.OutgoingTripsDashboardActivity" />
        <activity android:name=".PMsDashboardPackage.PMOutGoingTripsPackage.OutgoingDoneTripsActivity" />
        <activity android:name=".PMsDashboardPackage.PMOutGoingTripsPackage.OutgoingNotDoneTripsActivity" />
        <activity android:name=".PMsDashboardPackage.PMIncomingTripsPackage.IncomingTripsDashboardActivity" />
        <activity android:name=".PMsDashboardPackage.PMIncomingTripsPackage.IncomingDoneTripsActivity" />
        <activity android:name=".PMsDashboardPackage.PMIncomingTripsPackage.IncomingNotDoneTripsActivity" />
        <activity android:name=".MerchantsDashboardPackage.MerchantsOrdersOutside.UnPickedOrdersOutsideActivity" />
        <activity android:name=".MerchantsDashboardPackage.MerchantsOrdersOutside.PickedOrdersOutsideActivity" />
        <activity android:name=".MerchantsDashboardPackage.MerchantsOrdersOutside.NewOrdersOutsideActivity" />
        <activity android:name=".MerchantsDashboardPackage.MerchantsOrdersOutside.MerchantDashboardOutsideActivity" />
        <activity android:name=".MerchantsDashboardPackage.MerchantsOrdersOutside.DeliveredOrdersOutsideActivity" />
        <activity android:name=".EWalletPackage.MerchantsEWalletActivity" />
        <activity android:name=".Utilities.TermsAndCondActivity" />

        <service
            android:name=".Utilities.MyFirebaseMessagingService"
            android:stopWithTask="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

    </application>

</manifest>